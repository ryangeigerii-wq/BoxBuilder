{
    "name": "boxbuilder",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "lint": "eslint \"app/static/js/**/*.{js,jsx,mjs,cjs}\"",
        "format": "prettier --write \"app/static/js/**/*.{js,jsx,mjs,cjs}\"",
        "lint:fix": "eslint --fix \"app/static/js/**/*.{js,jsx,mjs,cjs}\"",
        "prepare": "husky",
        "test:js": "node app/static/js/tests/port_math.test.js",
        "test": "jest --runInBand"
    },
    "devDependencies": {
        "eslint": "^9.11.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "husky": "^9.1.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^30.2.0",
        "lint-staged": "^15.2.2",
        "prettier": "^3.2.5"
    },
    "lint-staged": {
        "app/static/js/**/*.{js,jsx,mjs,cjs}": [
            "eslint --fix",
            "prettier --write"
        ]
    }
}
